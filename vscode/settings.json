{
    "files.simpleDialog.enable": true,
    "editor.wordWrap": "on",
    "debug.openDebug": "neverOpen",
    "editor.tabCompletion": "on",
    "powershell.integratedConsole.focusConsoleOnExecute": false,
    "files.defaultLanguage": "powershell",
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
    "powershell.buttons.showPanelMovementButtons": true,
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "git.ignoreMissingGitWarning": true,
    "files.autoSave": "afterDelay",
    "editor.suggestSelection": "first",
    "git.enableSmartCommit": true,
    "diffEditor.renderSideBySide": true,
    "editor.inlineSuggest.enabled": true,
    "explorer.confirmDelete": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false,
        "yaml": true,
        "clojure": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "[python]": {
        "editor.formatOnType": true
    },
    "git.openRepositoryInParentFolders": "never",
    "workbench.activityBar.location": "bottom",
    "editor.wordBasedSuggestions": "off",
    "github.copilot.editor.enableCodeActions": false,
    "github.copilot.chat.terminalChatLocation": "terminal",
    "[markdown]": {
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "on"
        }
    },
    "notebook.lineNumbers": "on",
    "editor.fontLigatures": true,
    "terminal.integrated.enableImages": true,
    "accessibility.hideAccessibleView": true,
    "files.associations": {
        "*.ps1": "powershell",
        "*.clj": "clojure"
    },
    "editor.minimap.enabled": false,
    "terminal.integrated.localEchoExcludePrograms": [
        "vim",
        "vi",
        "nano",
        "tmux",
        "nvim"
    ],
    "powershell.integratedConsole.startLocation": "Editor",
    "terminal.integrated.ignoreProcessNames": [
        "oh-my-posh",
        "bash",
        "zsh"
    ],
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "MesloLGS Nerd Font Mono",
    "terminal.integrated.env.windows": {},
    "workbench.editor.showTabs": "single",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "zenMode.showTabs": "none",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.defaultProfile.windows": "Ubuntu-22.04 (WSL)",
    "terminal.integrated.confirmOnKill": "never",
    "terminal.integrated.defaultProfile.linux": "bash (2)",
    "terminal.integrated.hideOnStartup": "whenEmpty",
    "terminal.integrated.initialHint": false,
    "terminal.integrated.sendKeybindingsToShell": false,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiBlack",
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {},
    "[clojure]": {
        "diffEditor.ignoreTrimWhitespace": false
    },
    "notebook.confirmDeleteRunningCell": false,
    "python.REPL.enableREPLSmartSend": false,
    "workbench.editor.restoreViewState": false,
    "workbench.editor.decorations.colors": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "terminal.integrated.enablePersistentSessions": false,
    "powershell.codeFolding.showLastLine": false,
    "powershell.sideBar.CommandExplorerVisibility": true,
    "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.startAsLoginShell.linux": true,
    "powershell.startAsLoginShell.osx": false,
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.altClickMovesCursor": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "underline",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.suggest.enabled": true,
    "powershell.cwd": "~/workspace",
    "powershell.promptToUpdatePowerShell": false,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
    "powershell.codeFormatting.preset": "Stroustrup",
    "powershell.enableProfileLoading": false,
    "powershell.integratedConsole.startInBackground": true,
    "powershell.integratedConsole.suppressStartupBanner": true,
